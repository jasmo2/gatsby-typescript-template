{
  "name": "lambda-forms",
  "description": "The best way to handle forms on static websites.",
  "version": "1.0.0",
  "author": "@mattersupply.co",
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@material-ui/styles": "^4.7.1",
    "@types/node": "^12.11.2",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "dotenv": "^8.2.0",
    "gatsby": "^2.17.1",
    "gatsby-plugin-emotion": "^4.1.18",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-typescript": "^2.1.15",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "gatsby, typescript, canyoncoolers"
  ],
  "license": "MIT",
  "scripts": {
    "start": "yarn run develop",
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop --host 0.0.0.0 --port 8000",
    "refresh": "curl -X POST http://localhost:8000/__refresh",
    "build": "gatsby build",
    "serve": "gatsby serve --port 8000",
    "format": "prettier --parser typescript --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.ts*\""
  },
  "devDependencies": {
    "prettier": "^1.8.2"
  }
}
